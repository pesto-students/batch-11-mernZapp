    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jest": "^24.9.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
