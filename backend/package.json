{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "description": "backend for mernZapp",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run prod",
    "server": "babel-node -r ../node_modules/dotenv/config ./src/index.js dotenv_config_path=./config/dev.env",
    "server:prod": "babel-node ./src/index.js",
    "server:startOnHeroku": "node ./dist/index.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "precommit": "lint-staged"
  },
  "author": "batch-11-mernsters",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.1"
  },
  "dependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@slack/events-api": "^2.3.0",
    "@slack/web-api": "^5.6.0",
    "bcrypt": "^3.0.7",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.13",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "smee-client": "^1.1.0",
    "validator": "^12.1.0"
  },
  "lint-staged": {
    "!(*.test).js": [
      "eslint"
    ]
  }
}
